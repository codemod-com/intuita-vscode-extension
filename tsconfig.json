{
	"compilerOptions": {
        "experimentalDecorators": true,
		"module": "commonjs",
		"target": "ES2020",
		"lib": [
			"ES2020"
		],
		"sourceMap": false,
		"rootDir": "src",
		"strict": true,
		"noUncheckedIndexedAccess": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true
	}
}
