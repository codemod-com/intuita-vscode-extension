{
	"name": "intuita-vscode-extension",
	"displayName": "intuita-vscode-extension",
	"description": "Intuita VSCode Extension",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/*.ts*"
	],
	"main": "./dist/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts",
		"test": "mocha"
	},
	"devDependencies": {
		"@types/chai": "^4.3.1",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"chai": "^4.3.6",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"ts-loader": "^9.3.0",
		"ts-node": "^10.8.1",
		"umd-compat-loader": "^2.1.2",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"code-block-writer": "^11.0.0",
		"ts-morph": "^15.1.0",
		"typescript": "^4.7.3"
	}
}
